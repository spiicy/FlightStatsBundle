{
    "name": "spiicy/flightstats-bundle",
    "type": "symfony-bundle",
	"version": "1.2.3",
    "description": "This Bundle provides a wrapper for the FlightStats API",
    "keywords": ["rest", "api", "flight", "flightstats"],
    "homepage": "http://spiicy.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Veyrat",
            "email": "emmanuel@spiicy.io"
        }
    ],

    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": ">=2.7||~3.0||~4.0",
        "guzzlehttp/guzzle": "^6.2"
    },

    "autoload": {
        "psr-0": { "Spiicy\\Bundle\\FlightStatsBundle": "" }
    },

    "target-dir": "Spiicy/Bundle/FlightStatsBundle",

    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}