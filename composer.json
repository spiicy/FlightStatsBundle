{
    "name": "spiiicy/flightstats-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle provides a wrapper for the FlightStats API",
    "keywords": ["rest", "api", "flight", "flightstats"],
    "homepage": "http://spiicy.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Veyrat",
            "email": "emmanuel@spiicy.io"
        }
    ],

    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "2.*",
        "guzzlehttp/guzzle": "5.3"
    },

    "autoload": {
        "psr-0": { "spiiicy\\Bundle\\FlightStatsBundle": "" }
    },

    "target-dir": "spiiicy/Bundle/FlightStatsBundle",

    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}